sName		    star		          # Body's name
saModules   eqtide    # Modules

# Physical Parameters
dMass		    0.1945        # Mass, negative -> Earth masses
dAge        4.0e6       # Assume star is 1 Myr old when disk disperses
dRadGyra	  0.4	        # Radius of gyration (moment of inertia constant)
dRotPeriod  -122        # Rotation period, negative -> Days

# The first body in the saBodyFiles list must be the central mass.
# This body carries no orbital information => the coordinate system is bodycentric.

# EQTIDE Parameters
dTidalTau       -0.1        # Negative for seconds
dK2		        0.5		    # Love number of degree 2
sTideModel	  t8		    # Tidal model, p2=CPL, t8=CTL
saTidePerts	  b		# Body name(s) of tidal perturbers

saOutputOrder	Time -RotPer -TotEn -TotAngMom
